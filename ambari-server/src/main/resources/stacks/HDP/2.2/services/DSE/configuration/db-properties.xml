<configuration supports_final="false">
    <property>
        <name>dse.mysql.user</name>
        <value>ambari</value>
        <description>DSE配置库使用的Mysql用户名</description>
    </property>

    <property>
        <name>dse.mysql.password</name>
        <value>123456</value>
        <description>Mysql密码</description>
    </property>

  <property>
    <name>dse.mysql.port</name>
    <value>3306</value>
    <description>Mysql端口</description>
  </property>

  <property>
    <name>mysql.data.dir</name>
    <value>/dse/mysql_data</value>
    <description>Mysql数据目录</description>
  </property>
</configuration>
