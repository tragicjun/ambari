<configuration supports_final="false">

    <property>
        <name>server.port</name>
        <value>8085</value>
        <description>API Server端口</description>
    </property>
    <property>
        <name>management.port</name>
        <value>8088</value>
        <description>API Server Web端口</description>
    </property>
    <property>
        <name>spring.datasource.url</name>
        <value>jdbc:postgresql://127.0.0.1:5432/gaia_portal</value>
        <description>Ddatabase连接字符串</description>
    </property>
    <property>
        <name>spring.datasource.username</name>
        <value>postgres</value>
        <description>Database用户名</description>
    </property>
    <property>
        <name>spring.datasource.password</name>
        <value>postgres</value>
        <description>Database的密码</description>
    </property>
    <property>
        <name>spring.jpa.hibernate.ddl-auto</name>
        <value>update</value>
        <description>初始化Ddatabase时设为update，使用已有Database时设为validate</description>
    </property>
    <property>
        <name>docker.default.port</name>
        <value>2375</value>
        <description>Docker默认端口号</description>
    </property>
</configuration>
