<configuration>
  <property>
    <name>redis_log_file</name>
    <value>/var/log/redis/redis.log</value>
    <description></description>
  </property>
  <property>
    <name>redis_pid_file</name>
    <value>/var/run/redis/redis.pid</value>
    <description></description>
  </property>
  <property>
    <name>redis_port</name>
    <value>6379</value>
    <description></description>
  </property>
  <property>
    <name>redis_conf_dir</name>
    <value>/etc</value>
    <description></description>
  </property>
  <property>
    <name>redis_user</name>
    <value>redis</value>
    <property-type>USER</property-type>
  </property>
  <property>
    <name>redis_group</name>
    <value>redis</value>
    <property-type>GROUP</property-type>
  </property>
</configuration>
